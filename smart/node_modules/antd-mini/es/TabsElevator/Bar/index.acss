.amd-tabs-elevator-bar{padding:0 12px;background:#fff;position:sticky}.amd-tabs-elevator-bar::after{content:"";position:absolute;background-color:#eee;display:block;top:auto;right:0;bottom:0;left:0;height:1px;transform:scaleY(.5)}.amd-tabs-elevator-bar-mask{position:absolute;top:0;bottom:1px;width:40px;background:#fff;z-index:99;pointer-events:none}.amd-tabs-elevator-bar-mask-left{left:12px;mask-image:linear-gradient(to right,#000,rgba(255,255,255,0))}.amd-tabs-elevator-bar-mask-right{right:12px;mask-image:linear-gradient(to left,#000,rgba(255,255,255,0))}.amd-tabs-elevator-bar-items{display:flex;flex-wrap:nowrap;width:100%}.amd-tabs-elevator-bar-items::-webkit-scrollbar,.amd-tabs-elevator-bar-items::-webkit-scrollbar-thumb,.amd-tabs-elevator-bar-items::-webkit-scrollbar-track{display:none;opacity:0}.amd-tabs-elevator-bar-item{flex:0 0 auto;white-space:nowrap;padding-right:30px}.amd-tabs-elevator-bar-item:nth-last-of-type(1){padding-right:0}.amd-tabs-elevator-bar-item-wrapper{height:44px;line-height:44px;font-family:PingFangSC-Regular;font-size:15px;color:#333;text-align:center;position:relative}.amd-tabs-elevator-bar-item-wrapper-active{font-family:PingFangSC-Medium;color:#1677ff}.amd-tabs-elevator-bar-item-wrapper-active::after{content:'';position:absolute;left:0;right:0;bottom:1px;height:2px;background:#1677ff;border-radius:1px 1px 0 0}