.amd-vtabs{width:100vw;height:100vh;display:flex;flex-direction:row}.amd-vtabs-bar{width:105px;overflow:hidden}.amd-vtabs-bar-scroll-view{display:flex;flex-direction:column;width:105px;height:100%;background-color:#f5f5f5}.amd-vtabs-bar-scroll-view::-webkit-scrollbar{display:none}.amd-vtabs-bar-item{position:relative;width:105px;padding:16px 12px;overflow:hidden;display:flex;align-items:center;font-size:13px;line-height:18.5px;color:#333;background-color:#f5f5f5;box-sizing:border-box;transition:background-color .1s linear,color .3s linear}.amd-vtabs-bar-item-wrap{background-color:#fff}.amd-vtabs-bar-item-active{color:#1677ff;background-color:#fff}.amd-vtabs-bar-item-active::after{position:absolute;content:"";top:16px;bottom:16px;left:0;width:2px;border-radius:2px;background-color:#1677ff}.amd-vtabs-bar-item-disabled{pointer-events:none;cursor:not-allowed}.amd-vtabs-bar-item-disabled .amd-vtabs-bar-item-count,.amd-vtabs-bar-item-disabled .amd-vtabs-bar-item-title{opacity:.4}.amd-vtabs-bar-item-count{color:#ccc;margin-left:4px}.amd-vtabs-bar-item-icon{padding-left:4px;width:14px;height:14px;font-size:13px;overflow:hidden;position:relative;top:-2px;display:flex;justify-content:center;align-items:center}.amd-vtabs-bar-item-icon:empty{display:none}.amd-vtabs-content{overflow:hidden;flex:1;height:100%;background-color:#fff}.amd-vtabs-content-slides{height:100%}