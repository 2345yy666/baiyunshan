.amd-toast-wrapper{position:fixed;top:50%;left:50%;transform:translate3d(-50%,-50%,0);z-index:999;color:#fff;padding:12px;opacity:.75;background:#000;border-radius:8px;font-family:PingFangSC-Regular;position:absolute;display:flex;flex-direction:column;justify-content:center;align-items:center}.amd-toast-wrapper .amd-toast-icon,.amd-toast-wrapper .amd-toast-image,.amd-toast-wrapper .amd-toast-normal{margin-bottom:8px}.amd-toast-wrapper .amd-toast-normal{height:40px}.amd-toast-wrapper .amd-toast-icon{display:flex;justify-content:center;align-items:center}.amd-toast-wrapper .amd-toast-icon,.amd-toast-wrapper .amd-toast-image{width:40px;height:40px;background-size:contain;background-repeat:no-repeat}.amd-toast-wrapper .amd-toast-text-body{display:flex;width:190px;max-height:42px;justify-content:center}.amd-toast-wrapper .amd-toast-text-box{min-width:0;max-height:42px}.amd-toast-wrapper .amd-toast-text-content{font-size:15px;line-height:21px;max-width:100%;overflow:hidden;text-overflow:ellipsis}.amd-toastWithIcon-wrapper{width:140px;height:140px;display:flex;align-items:center;justify-content:center;flex-direction:column}