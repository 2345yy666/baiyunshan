.amd-skeleton-input{width:200px;height:20px;border-radius:4px;background:rgba(0,0,0,.06);display:inline-block}.amd-skeleton-input-animate{background:linear-gradient(90deg,rgba(190,190,190,.2) 25%,rgba(129,129,129,.24) 37%,rgba(190,190,190,.2) 63%);background-size:400% 100%;animation:amd-skeleton-loading 1.4s ease infinite}