@keyframes amd-skeleton-loading{0%{background-position:100% 50%}100%{background-position:0 50%}}.amd-skeleton-paragraph-animate .amd-skeleton-paragraph-row{background:linear-gradient(90deg,rgba(190,190,190,.2) 25%,rgba(129,129,129,.24) 37%,rgba(190,190,190,.2) 63%);background-size:400% 100%;animation:amd-skeleton-loading 1.4s ease infinite}.amd-skeleton-paragraph-row{width:100%;height:15px;margin-bottom:12px;border-radius:4px;background:rgba(0,0,0,.06)}.amd-skeleton-paragraph-row:nth-last-of-type(1){margin-bottom:0}.amd-skeleton-paragraph-row:nth-last-of-type(1):not(:nth-of-type(1)){width:60%}