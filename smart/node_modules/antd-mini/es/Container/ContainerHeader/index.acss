.amd-container-header{position:relative;height:46.5px;display:flex;align-items:center;justify-content:space-between;padding:0 12px}.amd-container-header-title{font-size:16px;font-weight:700;color:#333;display:flex;align-items:center;height:22.5px}.amd-container-header-image{width:24px;height:24px;margin-right:8px;flex-shrink:0}.amd-container-headerInBox{background-color:#fff;border-radius:8px 8px 0 0}.amd-container-headerInBox::after{content:"";position:absolute;background-color:#eee;display:block;top:auto;right:12px;bottom:0;left:12px;height:1px;transform:scaleY(.5)}