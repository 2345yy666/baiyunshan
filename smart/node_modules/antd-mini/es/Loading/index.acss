.amd-loading-spin{display:flex;justify-content:center;align-items:center;flex-direction:column;padding:12px}.amd-loading-spin-container{display:inline-block}.amd-loading-spin-icon{background-position:center;background-repeat:no-repeat}.amd-loading-spin-icon-small{width:17.5px;height:17.5px}.amd-loading-spin-icon-medium{width:35px;height:35px}.amd-loading-spin-icon-large{width:52.5px;height:52.5px}.amd-loading-spin-icon-x-large{width:70px;height:70px}.amd-loading-spin-icon-indicator:empty{display:none}.amd-loading-spin-icon+.amd-loading-spin-icon{display:none}.amd-loading-spin-icon-indicator:empty+.amd-loading-spin-icon{display:initial}.amd-loading-spin-icon-light{opacity:.6}.amd-loading-spin-text{padding:8px 0;text-align:center;font-size:11px;color:#454955}.amd-loading-spin-text-light{color:rgba(255,255,255,.6)}.amd-loading-mini{display:flex;justify-content:center;align-items:center;width:4px;height:4px;min-width:33px;min-height:33px}.amd-loading-mini-item{flex:0 0 4px;min-width:3px;min-height:3px;max-width:4px;max-height:4px;overflow:hidden;margin-right:8px;font-size:0;border-radius:2px/2;background-color:#999;animation:amd-loading-animation 1s 0s infinite linear}.amd-loading-mini-item__1{animation-delay:0s}.amd-loading-mini-item__2{animation-delay:150ms}.amd-loading-mini-item__3{margin-right:0;animation-delay:.3s}@keyframes amd-loading-animation{0%{transform:translate3d(0,0,0)}12%{transform:translate3d(0,-150%,0)}40%{transform:translate3d(0,110%,0)}55%{transform:translate3d(0,-10%,0)}60%{transform:translate3d(0,0,0)}100%{transform:translate3d(0,0,0)}}