.amd-guide-tour-button{width:100%;position:absolute;left:0;bottom:calc(100 * .5px + env(safe-area-inset-bottom));z-index:10001;display:flex;justify-content:center}.amd-guide-tour-button .amd-button:nth-of-type(1)>button{color:#fff;background-color:transparent}.amd-guide-tour-button .amd-button{margin:0 6px}.amd-guide-tour-indicator{width:100%;position:absolute;bottom:calc(calc(100 * .5px + env(safe-area-inset-bottom)) + 40 * .5px + 26px);left:50%;transform:translateX(-50%);z-index:10000;display:flex;justify-content:center}.amd-guide-tour-indicator-dot{margin:0 1.5px;background-color:#979797;width:3px;height:3px;border-radius:1px}.amd-guide-tour-indicator-dot-active{width:13px;background-color:#fff}.amd-guide-tour-clear{position:fixed;top:40px;right:20px;z-index:10001;font-size:28px;color:#999}.amd-guide-tour-item{z-index:10000;position:fixed;top:0;left:0}.amd-guide-tour-swiper{z-index:10000;position:fixed;top:0;left:0}