.amd-slider{--fill-color:#1677ff;user-select:none;width:100%}.amd-slider-track{position:relative;width:100%;height:28px;background-color:#fff}.amd-slider-track-number{height:48.5px}.amd-slider-track-touch-area{position:absolute;left:14px;top:0;right:12.5px;height:100%}.amd-slider-track-fill{position:absolute;left:0;top:12.5px;right:0;height:3px}.amd-slider-track-fill-background{position:absolute;background-color:#f5f5f5;width:100%;height:100%;border-radius:1.5px}.amd-slider-track-fill-front{position:absolute;height:100%;border-radius:1.5px;background-color:var(--fill-color)}.amd-slider-handler{position:absolute;left:0;width:28px;height:28px;transform:translate(-50%,-50%);top:1.5px;background:#fff;box-shadow:0 2px 5px 0 rgba(0,0,0,.12);border-radius:28px;text-align:center;line-height:28px;color:var(--fill-color)}.amd-slider-handler-icon-default{position:absolute;top:0;left:0;right:0;bottom:0}.amd-slider-handler-icon-default-line1{position:absolute;width:2px;height:8px;background-color:var(--fill-color);border-radius:1px;top:10px;left:8px}.amd-slider-handler-icon-default-line2{position:absolute;width:2px;height:12px;background-color:var(--fill-color);border-radius:1px;top:8px;right:13px}.amd-slider-handler-icon-default-line3{position:absolute;width:2px;height:8px;background-color:var(--fill-color);border-radius:1px;top:10px;right:8px}.amd-slider-handler-icon-from-props{position:absolute;top:0;left:0;right:0;bottom:0;text-align:center;line-height:100%}.amd-slider-handler-icon-from-props-icon{position:absolute;left:50%;top:50%;transform:translate(-50%,-50%)}.amd-slider-tick{position:absolute;width:8px;height:8px;border-radius:8px;top:1.5px;transform:translate(-50%,-50%)}.amd-slider-tick-front{background-color:var(--fill-color)}.amd-slider-tick-back{background-color:#f5f5f5}.amd-slider-tick-number{position:absolute;color:#333;font-size:12px;transform:translateX(-50%);top:22px;left:4px;text-align:center;line-height:16.5px;height:16.5px}.amd-slider-tooltip .amd-popover-content{top:-13px;min-width:0}.amd-slider-tooltip .amd-popover-content .amd-popover-inner{padding:0 12px}.amd-slider-tooltip-content{font-size:15px;color:#fff}