.amd-tag{position:relative;display:inline-flex;align-items:center;padding:1px 4px;font-size:11px;line-height:15px;box-sizing:border-box;border-radius:2px;color:#fff}.amd-tag-icon-container{margin-right:3px;display:flex;justify-content:center;align-self:center;align-items:center}.amd-tag-icon-container .amd-icon{font-size:12px}.amd-tag-primary{background-color:#1677ff}.amd-tag-primary.amd-tag-fill-light{color:#1677ff;background-color:#e7f1ff}.amd-tag-primary.amd-tag-outline{color:#1677ff;border-color:rgba(22,119,255,.3)}.amd-tag-warn{background-color:#ff8f1f}.amd-tag-warn.amd-tag-fill-light{color:#ff8f1f;background-color:#ffefdf}.amd-tag-warn.amd-tag-outline{color:#ff8f1f;border-color:rgba(255,143,31,.3)}.amd-tag-danger{background-color:#ff6010}.amd-tag-danger.amd-tag-fill-light{color:#ff6010;background-color:#ffece3}.amd-tag-danger.amd-tag-outline{color:#ff6010;border-color:rgba(255,96,16,.3)}.amd-tag-success{background-color:#00b578}.amd-tag-success.amd-tag-fill-light{color:#00b578;background-color:#d4fff1}.amd-tag-success.amd-tag-outline{color:#00b578;border-color:rgba(0,181,120,.3)}.amd-tag-image{background-color:rgba(0,0,0,.5)}.amd-tag-outline{border-width:2rpx;border-style:solid;background-color:#fff}.amd-tag-content{display:flex;justify-content:center;align-items:center}