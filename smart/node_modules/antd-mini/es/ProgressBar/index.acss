.amd-progressbar-wrapper{display:flex;flex-direction:row;align-items:center}.amd-progress-outer{background-color:#e5e5e5;border-radius:999999px;height:8px;overflow:hidden;flex:1}.amd-progress-inner{border-radius:999999px;transition:width .3s;height:100%;position:relative}.amd-progress-indicator{line-height:18.5px;margin-left:8px;font-family:PingFangSC-Regular;font-size:13px;color:#999}.amd-progress-indicator .amd-progress-type{width:12px;height:12px}