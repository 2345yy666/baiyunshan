.amd-notice-bar{position:relative;display:flex;height:37.5px;align-items:center;overflow:hidden;padding:8px 12px;font-size:15px;color:#ff6010;background-color:#fff9ed;border:1px solid #fff3d9;box-sizing:border-box}.amd-notice-bar-error{color:#fff;background-color:#ff3b30;border:1px solid #d9281e}.amd-notice-bar-error-scroll-left,.amd-notice-bar-error-scroll-right{background:#ff3b30}.amd-notice-bar-primary{color:#1677ff;background-color:#d0e4ff;border:1px solid #bed9ff}.amd-notice-bar-primary-scroll-left,.amd-notice-bar-primary-scroll-right{background:#d0e4ff}.amd-notice-bar-info{color:#fff;border:1px solid #999;background:#ababab}.amd-notice-bar-info-scroll-left,.amd-notice-bar-info-scroll-right{background:#ababab}.amd-notice-bar-icon{margin-right:8px;font-size:18px}.amd-notice-bar-content{position:relative;z-index:2;flex:1 100%;overflow:hidden;vertical-align:middle;line-height:1.4}.amd-notice-bar-marquee{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;vertical-align:middle}.amd-notice-bar-operation{display:flex;align-items:center}.amd-notice-bar-operation-icon{margin-left:12px}.amd-notice-bar-operation-text{margin-left:12px;white-space:nowrap}.amd-icon-size-x-small{font-size:18px}