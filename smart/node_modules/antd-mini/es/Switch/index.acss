@keyframes loading-rotate{100%{transform:rotate(1turn)}}.amd-switch{display:inline-block;vertical-align:middle;position:relative;align-self:center;min-width:51px;height:31px;box-sizing:border-box;border:2px solid #e5e5e5;border-radius:15.5px;background:#fff;overflow:hidden;line-height:31px}.amd-switch-checked{background:#1677ff;border-color:#1677ff}.amd-switch-checked .amd-switch-handle{left:calc(100% - (31px - 2 * 2px))}.amd-switch-checked .amd-switch-inner{color:#fff;margin:0 32px 0 8px}.amd-switch-disabled{opacity:.4;pointer-events:none}.amd-switch-loading{pointer-events:none}.amd-switch-loading-icon{font-size:10px;animation:loading-rotate 1s linear infinite;color:#1677ff}.amd-switch-handle{display:flex;justify-content:center;align-items:center;width:27px;height:27px;border-radius:50%;background:#fff;position:absolute;z-index:2;top:0;left:0;transition:all .2s;box-shadow:0 0 2px 0 rgba(0,0,0,.2),0 2px 11.5px 0 rgba(0,0,0,.08),-1px 2px 2px 0 rgba(0,0,0,.1)}.amd-switch-inner{position:relative;z-index:1;display:flex;justify-content:center;align-items:center;margin:0 5px 0 32px;height:100%;color:#999;transition:margin .2s;font-size:15px}.amd-switch-small{min-width:36px;height:21px;border-radius:10.5px}.amd-switch-small .amd-switch-handle{width:17px;height:17px}.amd-switch-small .amd-switch-inner{margin:0 5px 0 22px}.amd-switch-small.amd-switch-checked .amd-switch-handle{left:calc(100% - (21px - 2 * 2px))}.amd-switch-small.amd-switch-checked .amd-switch-inner{color:#fff;margin:0 22px 0 8px}.amd-switch-x-small{min-width:26px;height:16px;border-radius:8px}.amd-switch-x-small .amd-switch-handle{width:12px;height:12px}.amd-switch-x-small .amd-switch-inner{margin:0 5px 0 17px}.amd-switch-x-small.amd-switch-checked .amd-switch-handle{left:calc(100% - (16px - 2 * 2px))}.amd-switch-x-small.amd-switch-checked .amd-switch-inner{color:#fff;margin:0 17px 0 8px}